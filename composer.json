{
    "name": "baagee/hellobase",
    "description": "Hbase client php wrapper, via thirft. php使用thirft操作hbase库",
    "keywords": [
        "hbase",
        "thirft"
    ],
    "config": {
        "secure-http": false
    },
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "HelloBase\\": "src",
            "Thrift\\": "bridge/Thrift"
        },
        "classmap": [
            "bridge/Hbase"
        ]
    }
}
